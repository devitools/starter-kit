{
    "name": "devitools/replace.app.short",
    "type": "project",
    "description": "The best app ever",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-zip": "*",
        "coraxster/flysystem-aws-s3-v3-minio": "^1.0",
        "doctrine/dbal": "^3.1",
        "dyrynda/laravel-efficient-uuid": "^4.3",
        "dyrynda/laravel-model-uuid": "^6.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "neitanod/forceutf8": "^2.0",
        "owen-it/laravel-auditing": "^12.0",
        "php-brasil/collection": "^0.2.5",
        "phpzm/phpee": "^1.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^4.0",
        "sentry/sentry-laravel": "^2.5",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Devitools\\": "@devitools/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "@devitools/src/Helper/index.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
