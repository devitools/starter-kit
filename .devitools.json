{
  "name": "Devitools App",
  "short": "devitools",
  "lang": "pt-br",
  "template": "default",
  "front": {
    "type": "quasar",
    "env": "yarn",
    "root": "/frontend",
    "domains": "/source/domains",
    "views": "/resources/views/dashboard",
    "settings": "/src",
    "routes": "/routes/dashboard",
    "i18n": "/resources/lang"
  },
  "back": {
    "type": "laravel",
    "env": "docker",
    "root": "/backend",
    "domains": "/app/Domains",
    "controllers": "/app/Http/Controllers",
    "migrations": "/database/migrations",
    "routes": "/routes/api"
  }
}